<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net7.0-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWindowsForms>true</UseWindowsForms>
		<ImplicitUsings>enable</ImplicitUsings>
		<PlatformTarget>AnyCPU</PlatformTarget>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Clojure" Version="1.12.0-alpha3" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="ClojureSrc.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>ClojureSrc.resx</DependentUpon>
		</Compile>
		<Compile Update="Schemas.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>Schemas.resx</DependentUpon>
		</Compile>
		<Compile Update="Settings.Designer.cs">
			<DesignTimeSharedInput>True</DesignTimeSharedInput>
			<AutoGen>True</AutoGen>
			<DependentUpon>Settings.settings</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Update="ClojureSrc.resx">
			<Generator>ResXFileCodeGenerator</Generator>
			<LastGenOutput>ClojureSrc.Designer.cs</LastGenOutput>
		</EmbeddedResource>
		<EmbeddedResource Update="Schemas.resx">
			<Generator>ResXFileCodeGenerator</Generator>
			<LastGenOutput>Schemas.Designer.cs</LastGenOutput>
		</EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
		<None Update="Resources\clojure\tbx_dialect_schema_generator\main.cljr">
			<CopyToOutputDirectory>Never</CopyToOutputDirectory>
		</None>
		<None Update="Settings.settings">
			<Generator>SettingsSingleFileGenerator</Generator>
			<LastGenOutput>Settings.Designer.cs</LastGenOutput>
		</None>
	</ItemGroup>

</Project>